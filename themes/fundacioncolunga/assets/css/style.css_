body { font-family: 'Catamaran', sans-serif; font-size: 16px; background-color: #FFFFFF; color: #32423B; font-weight: 200; }
ul { display: inline-block; vertical-align: top; list-style: none; padding-left: 0; }
a { text-decoration: none; cursor: pointer; font-size: 18px; transition: all 0.3s ease-out; -webkit-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out; -moz-transition: all 0.3s ease-out; -ms-transition: all 0.3s ease-out; outline:none; }
a:hover , a:focus {  text-decoration: none; }
img { margin: 0 auto; max-width: 100%; }
section { padding: 30px 0;}
h1,h2,h3,h4,h5,h6 { margin: 0; font-weight: initial; }
h1 {  font-size: 28px; line-height: 34px; color: #32423B; margin-bottom: 15px; }
h4 {line-height: 24px; }
input { font-size: 18px; }
.form-control:focus, input:focus { outline: none; border-color: transparent; box-shadow: none; }
a.no-style,
a.no-style:hover,
a.no-style:active,
a.no-style:visited { color: inherit; text-decoration: none; font-size: inherit; }
iframe { width: 100%; height: 400px; max-width: 100%; }
label { font-weight: 400; font-size: 18px !important; }
/* WP Images */
img.alignright { float: right; margin: 0 0 1em 1em; }
img.alignleft { float: left; margin: 0 1em 1em 0; }
img.aligncenter { display: block; margin-left: auto; margin-right: auto; }
.alignright { float: right; }
.alignleft { float: left; }
.aligncenter { display: block; margin-left: auto; margin-right: auto; }
