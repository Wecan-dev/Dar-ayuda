/*fixed menu*/
#collapse-menu {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background: #36453E;
  z-index: 200;
  color: #ffffff;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  -webkit-transform: translateY(-100%);
  -moz-transform: translateY(-100%);
  -ms-transform: translateY(-100%);
  -o-transform: translateY(-100%);
  transform: translateY(-100%);
  -webkit-transition-property: -webkit-transform;
  -moz-transition-property: -moz-transform;
  transition-property: transform;
  -webkit-transition-duration: 0.4s;
  -moz-transition-duration: 0.4s;
  transition-duration: 0.4s;

}
#collapse-menu.show  { -webkit-transform: translateY(0); -moz-transform: translateY(0); -ms-transform: translateY(0); -o-transform: translateY(0); transform: translateY(0); }
#collapse-menu .container { padding: 80px 15px; }
#collapse-menu ul { width: 48%; margin: 30px 0; }
#collapse-menu ul.list-filtro { width: 100%;}
#collapse-menu ul.list-filtro input { max-width: 400px; width: 100%;}
#collapse-menu .list-inicio { width: 100%; margin: 10px 0;}
