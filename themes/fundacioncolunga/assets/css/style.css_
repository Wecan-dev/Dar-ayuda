#collapse-menu ul li:first-of-type a,
#collapse-menu ul.list-dest li a { font-size: 24px; margin-bottom: 20px; font-weight: 500; color: #ffffff; }
#collapse-menu ul.list-dest { width: 100%; }
#collapse-menu ul.list-dest li a { margin-bottom: 10px; }
#collapse-menu li a { font-size: 18px; color: rgba(255,255,255,.7); font-weight: 300; }
#collapse-menu li a:hover { color:#ffffff; }

